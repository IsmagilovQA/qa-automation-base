<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="regress-back">
    <listeners>
        <listener class-name="main.java.listeners.RetryListener" />
        <listener class-name="main.java.listeners.EventListener" />
    </listeners>
    <test name="Regression for Frontend" >
        <groups>
            <run>
                <include name="auth"/>
            </run>
        </groups>

        <packages>
            <package name="test.java.frontend.*" />
        </packages>
    </test>
</suite>